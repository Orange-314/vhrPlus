<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.vhrdemo.dao.ISalaryDao">
    <resultMap id="BaseResultMap" type="com.example.vhrdemo.entity.Salary">
    </resultMap>

    <select id="findall" resultMap="BaseResultMap" parameterType="com.example.vhrdemo.entity.Salary">
        SELECT * from salary;
    </select>
    <select id="search" resultMap="BaseResultMap" parameterType="com.example.vhrdemo.entity.Salary">
        SELECT * from salary where name=#{name};
    </select>
    <select id="select" resultMap="BaseResultMap" parameterType="com.example.vhrdemo.entity.Salary">
        SELECT * from salary where id=#{id};
    </select>
    <insert id="insert1" parameterType="com.example.vhrdemo.entity.Salary">
        INSERT INTO salary VALUES (NULL,#{basicSalary},#{bonus},#{lunchSalary},#{trafficSalary},#{allSalary},NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL);
    </insert>
    <insert id="insert" parameterType="com.example.vhrdemo.entity.Salary">
        INSERT INTO salary VALUES (NULL,#{basicSalary},#{bonus},#{lunchSalary},#{trafficSalary},#{allSalary},#{pensionBase},#{pensionPer},NULL,#{medicalBase},#{medicalPer},#{accumulationFundBase},#{accumulationFundPer},#{name});
    </insert>
    <insert id="update" parameterType="com.example.vhrdemo.entity.Salary">
        UPDATE salary SET basicSalary=#{basicSalary},bonus=#{bonus},lunchSalary=#{lunchSalary},trafficSalary=#{trafficSalary},allSalary=#{allSalary},pensionBase=#{pensionBase},pensionPer=#{pensionPer},createDate=NULL,medicalBase=#{medicalBase},medicalPer=#{medicalPer},accumulationFundBase=#{accumulationFundBase},accumulationFundPer=#{accumulationFundPer},name=#{name} WHERE id=#{id};
    </insert>
    <delete id="delete" parameterType="com.example.vhrdemo.entity.Salary">
        DELETE FROM salary WHERE id=#{id};
    </delete>

</mapper>