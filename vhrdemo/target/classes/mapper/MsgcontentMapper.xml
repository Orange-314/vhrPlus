<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.vhrdemo.dao.IMsgcontentDao">
    <resultMap id="BaseResultMap" type="com.example.vhrdemo.entity.Msgcontent">
    </resultMap>
    <select id="search" resultMap="BaseResultMap" parameterType="com.example.vhrdemo.entity.Msgcontent">
        SELECT * from msgcontent where id=#{id}
    </select>
    <insert id="insert" parameterType="com.example.vhrdemo.entity.Msgcontent">
        INSERT INTO msgcontent (id,title,message) VALUES (#{id},#{title},#{message});
    </insert>
</mapper>